@page
@model XtendersProject.Quotaions.DetailsModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}



    <div class="card p-5">
        <h4>Quotation from @Model.Quotation.Name.ToUpper()</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Quotation.Name)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Quotation.Name)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Quotation.Email)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Quotation.Email)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Quotation.Detail)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Quotation.Detail)
            </dd>
        </dl>

        <div>
            <a asp-page="./Index" class="btn btn-outline-primary">Back to List</a>
        </div>
    </div>

